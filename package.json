{
  "name": "mongo2postman",
  "displayName": "MongoTest: Schema to Postman",
  "description": "Convert Mongoose schemas to Postman-ready JSON data for easy API testing. Streamline your MongoDB and Postman workflow.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Testing",
    "Formatters"
  ],
  "icon": "images/icon.jpeg",
  "keywords": [
    "Mongoose",
    "Postman",
    "API",
    "Schema",
    "Converter",
    "MongoDB",
    "Testing",
    "JSON"
  ],
  "homepage": "https://github.com/BamaCharanChhandogi/mongoose-to-postman",
  "bugs": {
  "url": "https://github.com/iamkaniska/mongoose-to-postman/issues",
  "email": "kaniskamaity@gmail.com"
},
  "author": {
  "name": "Kaniska Maity",
  "email": "kaniskamaity@gmail.com",
  "url": "https://github.com/iamkaniska"
},
  "publisher": "KaniskaMaity",
  "repository": {
    "type": "git",
    "url": "https://github.com/BamaCharanChhandogi/mongoose-to-postman.git"
  },
  "license": "MIT",
  "activationEvents": [
    "onCommand:mongoose-to-postman.convertToPostmanRaw"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mongoose-to-postman.convertToPostmanRaw",
        "title": "Convert to Postman Raw"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "mongoose-to-postman.convertToPostmanRaw",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
"devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3"
  }
}
